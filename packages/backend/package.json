  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.21",
    "@types/sequelize": "^4.28.19",
    "@types/supertest": "^6.0.2",
    "@types/cors": "^2.8.17",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
